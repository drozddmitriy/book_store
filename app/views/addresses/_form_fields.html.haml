= f.input :first_name, required: false
= f.input :last_name, required: false
= f.input :address, required: false
= f.input :city, required: false
= f.input :zip, required: false
= f.input :country, required: false, prompt: t('views.addresses.country')
= f.input :phone, required: false, placeholder: t('views.addresses.phone')
= f.input :cast, as: :hidden, input_html: { value: cast }
= f.button :submit, t('views.addresses.save'), class:"btn btn-default btn"
